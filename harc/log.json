{
    "version": 1,
    "formatters": {
        "standard": {
            "format": "%(asctime)s %(name)s %(levelname)s %(module)s %(funcName)s:%(lineno)s - %(message)s"
        }
    },
    "handlers": {
        "default": {
            "level": "DEBUG",
            "formatter": "standard",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "": {
            "handlers": ["default"],
            "level": "DEBUG",
            "propagate": true
        },
        "my.package": {
            "handlers": ["default"],
            "level": "DEBUG",
            "propagate": false
        }
    }
}